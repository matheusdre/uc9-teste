programa
{
	
	funcao inicio()
	{
		//vamos contar de 1 até 10
	inteiro contador = 1
	inteiro valor
	escreva("Você quer contar até quanto?")
	leia(valor)
	enquanto(contador <= valor){
	escreva(contador, "\n")
	contador++

	}

		escreva("Terminei de contar")
		
	}
}

